{
  "$schema": "https://ui.shadcn.com/schema/registry.json",
  "name": "acme",
  "homepage": "https://acme.com",
  "items": [
    {
      "name": "icon-picker",
      "type": "registry:ui",
      "title": "Icon Picker",
      "description": "An icon picker component with tabler icons.",
      "author": "emretuna",
      "files": [
        {
          "path": "registry/ui/icon-picker.tsx",
          "type": "registry:component",
          "target": "components/ui/icon-picker.tsx"
        },
        {
          "path": "registry/ui/icons-data.ts",
          "type": "registry:component",
          "target": "components/ui/icons-data.ts"
        }
      ],
      "dependencies": [
        "@tabler/icons-react@^3.34.0",
        "@tanstack/react-virtual",
        "fuse.js",
        "usehooks-ts"
      ],
      "registryDependencies": [
        "button",
        "input",
        "popover",
        "tooltip",
        "skeleton"
      ]
    },
    {
      "name": "icon-picker-example",
      "type": "registry:ui",
      "title": "Icon Picker Example",
      "description": "An example of the icon picker component.",
      "author": "emretuna",
      "registryDependencies": [
        "https://github.com/emretuna/shadcn-iconpicker-tabler"
      ],
      "files": [
        {
          "path": "registry/icon-picker-example.tsx",
          "type": "registry:component",
          "target": "components/icon-picker-example.tsx"
        }
      ]
    }
  ]
}

